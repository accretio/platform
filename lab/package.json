    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "body-parser": "^1.17.1",
    "bootstrap": "^4.0.0-alpha.6",
    "cross-env": "^3.1.4",
    "css-loader": "^0.27.3",
    "ejs": "^2.5.1",
    "elasticsearch": "^12.1.3",
    "es6-promise": "^4.1.0",
