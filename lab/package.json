{
			"name": "platform.lab",
			"version": "0.0.1",
			"description": "Create or Import designs",
			"main": "src/server.js",
			"scripts": {
						"clean": "rm -rf build && mkdir build",
						"build-server": "babel --plugins=transform-es2015-modules-commonjs --presets react,es2015 -d ./build ./src/server.js -s",
						"start2": "node ./build/src/server.js",
						"start": "cross-env NODE_ENV=production node_modules/.bin/babel-node --plugins=transform-es2015-modules-commonjs --presets react,es2015 src/server.js",
						"start-api-dev": "cross-env NODE_ENV=development node_modules/.bin/babel-node --plugins=transform-es2015-modules-commonjs --presets react,es2015 src/server.js",
						"start-dev": "npm run start-dev-hmr",
						"start-dev-single-page": "node_modules/.bin/http-server src/static",
						"start-dev-hmr": "node_modules/.bin/webpack-dev-server --progress --inline --hot --open",
						"build": "cross-env NODE_ENV=production node_modules/.bin/webpack",
						"dockerize": "npm run build && docker build -t gcr.io/accretio-platform/lab ."
			},
			"resolve.symlinks": false,
			"author": "William Le Ferrand",
			"license": "GPL-3.0",
